{
  "name": "mini-apps",
  "version": "0.0.1",
  "description": "Mini Apps",
  "main": "index.js",
  "repository": "git@github.com:hieudoanm/mini-apps.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14",
    "npm": ">= 8",
    "yarn": "1"
  },
  "scripts": {
    "build:package:shared": "yarn workspace @package/shared build",
    "build:web:coins": "yarn workspace @web/coins build",
    "build:web:countries": "yarn workspace @web/countries build",
    "build:web:forex": "yarn workspace @web/forex build",
    "build:web:pomodoro": "yarn workspace @web/pomodoro build",
    "build:web:status": "yarn workspace @web/status build",
    "build": "yarn build:package:shared && yarn build:web:coins && yarn build:web:countries && yarn build:web:forex && yarn build:web:pomodoro && yarn build:web:status",
    "dev:web:coins": "yarn workspace @web/coins dev",
    "dev:web:countries": "yarn workspace @web/countries dev",
    "dev:web:forex": "yarn workspace @web/forex dev",
    "dev:web:pomodoro": "yarn workspace @web/pomodoro dev",
    "dev:web:status": "yarn workspace @web/status dev",
    "dev": "concurrently --kill-others-on-fail \"yarn dev:web:coins\" \"yarn dev:web:countries\" \"yarn dev:web:forex\" \"yarn dev:web:pomodoro\" \"yarn dev:web:status\"",
    "export:web:coins": "yarn workspace @web/coins export",
    "export:web:countries": "yarn workspace @web/countries export",
    "export:web:forex": "yarn workspace @web/forex export",
    "export:web:pomodoro": "yarn workspace @web/pomodoro export",
    "export:web:status": "yarn workspace @web/status export",
    "export": "yarn export:web:coins && yarn export:web:countries && yarn export:web:forex && yarn export:web:pomodoro && yarn export:web:status",
    "lint:package:shared": "yarn workspace @package/shared lint",
    "lint:web:coins": "yarn workspace @web/coins lint",
    "lint:web:countries": "yarn workspace @web/countries lint",
    "lint:web:forex": "yarn workspace @web/forex lint",
    "lint:web:pomodoro": "yarn workspace @web/pomodoro lint",
    "lint:web:status": "yarn workspace @web/status lint",
    "lint": "yarn lint:package:shared && yarn lint:web:coins && yarn lint:web:countries && yarn lint:web:forex && yarn lint:web:pomodoro && yarn lint:web:status",
    "test:package:shared": "yarn workspace @package/shared test",
    "test:web:coins": "yarn workspace @web/coins test",
    "test:web:countries": "yarn workspace @web/countries test",
    "test:web:forex": "yarn workspace @web/forex test",
    "test:web:pomodoro": "yarn workspace @web/pomodoro test",
    "test:web:status": "yarn workspace @web/status test",
    "test": "yarn test:package:shared && yarn test:web:coins && yarn test:web:forex && yarn test:web:pomodoro && yarn test:web:status"
  },
  "devDependencies": {
    "concurrently": "7.3.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3"
  },
  "workspaces": [
    "packages/*",
    "web/*"
  ]
}
